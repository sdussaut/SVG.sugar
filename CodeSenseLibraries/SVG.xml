		<completion string="svg">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\s*:)|[^:]</matched-suffix>
					<transform-into>(?1:: xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" baseProfile="full">${0}&lt;/svg>)</transform-into>
