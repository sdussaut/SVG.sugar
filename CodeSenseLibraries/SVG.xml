	<set name="com.walkingamonggiants.svg.tags.self-closing">
		<!-- A behavior defined in a set applies only to that set. -->
		<behavior>
			<append-static> /></append-static>
		</behavior>
		
		
		<!-- tags -->
		<!-- primitives -->
